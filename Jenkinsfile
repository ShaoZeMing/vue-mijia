#!/usr/bin/env groovy
pipeline {
    agent any
    stages {
        stage('Example') {
            steps {
                echo 'Hello World this is Example'
            }
        }
        stage('Test') {
            steps {
                echo 'Hello This is Test'

            }
        }
        stage('Deploy') {
            steps {
                echo 'Hello This is Deploy'
            }
        }
    }
}

